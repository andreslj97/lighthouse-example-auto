{
  "ci": {
    "collect": {
      "url": [
        "https://www.chedraui.com.mx/supermercado?workspace=betaplpload",
        "https://www.chedraui.com.mx/supermercado?workspace=currentmaster"
      ],
      "numberOfRuns": 5,
      "throttlingMethod": "simulate",
      "settings": {
        "formFactor": "desktop",
        "screenEmulation": {
          "mobile": false,
          "width": 1360,
          "height": 1020,
          "deviceScaleFactor": 1,
          "disabled": false
        },
        "throttling": {
          "rttMs": 28,
          "throughputKbps": 5000,
          "requestLatencyMs": 28,
          "downloadThroughputKbps": 5000,
          "uploadThroughputKbps": 1000
        }
      }
    },
    "assert": {
      "assertions": {
        "categories:performance": [
          "warn",
          {
            "minScore": 0.7
          }
        ]
      }
    },
    "upload": {
      "target": "filesystem",
      "outputDir": "./lighthouse-report",
      "format": ["html", "json"]
    }
  }
}